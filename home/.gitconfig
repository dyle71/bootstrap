# private gitconfig of dyle --> copy to ~/.gitconfig

[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --branches --remotes --tags --graph --oneline --decorate
    type = cat-file -t
    dump = cat-file -p

[user]
#    email = 
#    name = 

[color]
    branch = auto
    diff = auto
    grep = auto
    interactive = auto
    pager = true
    status = auto
    ui = auto

